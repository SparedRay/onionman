script/source = "extends Timer

var time_left = 0
var time_node

func _ready():
	# Called every time the node is added to the scene.
	# Initialization here
	time_left = get_wait_time()
	time_node = get_parent().get_node(\"TimeLeft\")
	time_node.set_text(\"%01d\" % (floor(time_left)))

func _process(delta):
	time_left = get_time_left()
	time_node.set_text(\"%01d\" % (floor(time_left)))
"

